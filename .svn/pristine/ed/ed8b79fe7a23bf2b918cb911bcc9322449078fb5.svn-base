                {
                    NSLbl.Text = DsLbl.Text.Substring(0, 6) + "N" + DsLbl.Text.Substring(7, 1);
                }

                if (DsLbl.Text.Substring(6, 1) == "U")
                {
                    NSLbl.Text = DsLbl.Text.Substring(0, 6) + "P" + DsLbl.Text.Substring(7, 1);
